   "source": [
    "from decimal import Decimal\n",
    "\n",
    "p = [0 for _ in range(10)]\n",
    "\n",
    "for i in range(10):\n",
    "    p[i] = float(input())\n",
    "\n",
    "p.sort(reverse=True)\n",
    "\n",
    "result = Decimal(1.0)\n",
    "\n",
    "for i in range(9):\n",
    "    result *= Decimal(p[i])/(i+1)\n",
    "\n",
    "print(result*(10**9))"
   ]
