   "source": [
    "A,B = map(int,input().split())\n",
    "\n",
    "\n",
    "for j in range(min(A,B),0,-1):\n",
    "    if A%j==0 and B%j==0:\n",
    "        print(j)\n",
    "        break\n",
    "\n",
    "for i in range(max(A,B),A*B+1):\n",
    "    if i%A==0 and i%B==0:\n",
    "        print(i)\n",
    "        break"
   ]
  }
