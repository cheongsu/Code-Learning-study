   "source": [
    "n = int(input())\n",
    "\n",
    "dp = [0 for i in range(31)]\n",
    "\n",
    "dp[2] = 3\n",
    "\n",
    "for i in range(4,31,2):\n",
    "    dp[i] = dp[2]*dp[i-2]\n",
    "    for j in range(4,i,2):\n",
    "        dp[i]+=2*dp[i-j]\n",
    "    dp[i]+=2\n",
    "\n",
    "\n",
    "\n",
    "print(dp[n])"
   ]
